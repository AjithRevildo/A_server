{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/fastest-urls", "dest": "/api/fastest-urls" },
    { "src": "/checkUrls", "dest": "/api/checkUrls" },
    { "src": "/api/.*", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
